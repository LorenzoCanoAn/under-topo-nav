<?xml version="1.0"?>
<launch>

  <!-- GAZEBO PARAMETERS-->
  <arg name="world_file_path" default="/home/lorenzo/catkin_ws/src/subt_gazebo/worlds/w2.world" />
  <!-- GALLERY DETECTION PARAMETERS-->
  <!-- Image to ptcl parameters-->
  <arg name="img_type" default="angles" />
  <arg name="image_width" default="360"/>
  <arg name="image_height" default="16"/>
  <!-- Neural Network Parameters-->
  <arg name="nn_type" default="gallery_detector_v3"/>
  <arg name="nn_path" default="/home/lorenzo/catkin_data/models/gallery_detection_nn_well_saved/gallery_detector_v3_loss_MSELoss_lr_0.0001_N_8_refined_state_dict"/>

  <include file="$(find tunnel_navigation_launch)/launch/simplest_env.launch">
    <arg name="world_file_path" value="$(arg world_file_path)" />
  </include>

  <include file="$(find pointcloud_to_image)/launch/ptcl_to_img.launch">
    <arg name="img_type" default="$(arg img_type)" />
    <arg name="image_width" default="$(arg image_width)"/>
    <arg name="image_height" default="$(arg image_height)"/> 
  </include>

  <include file="$(find teleop_twist_joy)/launch/teleop.launch"></include>

  <node type="rviz" name="rviz" pkg="rviz" args="-d $(find tunnel_navigation_launch)/rviz/default.rviz" />

  <include file="$(find gallery_detection)/launch/gallery_detection.launch">
    <arg name="nn_type" value="$(arg nn_type)"/>
    <arg name="nn_path" value="$(arg nn_path)"/>
  </include>

</launch>